{
    "manifest_version": 3,
    "name": "HackTheBox Walkthroughs",
    "version": "1.0.0",
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
      "tabs",
      "storage",
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["https://app.hackthebox.com/machines/*"],
        "js": ["content.js"],
        "css": ["styles.css"]
      }
    ]
  }
  